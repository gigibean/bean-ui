{
  "name": "@bean-ui/monorepo",
  "version": "0.0.0",
  "workspaces": {
    "packages": [
      "package/*"
    ]
  },
  "scripts": {
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx --fix --cache",
    "prebuild:all": "yarn workspaces foreach -p run prebuild",
    "dev:all": "yarn workspaces foreach -p run dev",
    "preview:all": "yarn workspaces foreach -p run preview"
  },
  "packageManager": "yarn@3.2.1",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.18.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.1.3",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4"
  },
  "resolutions": {
    "@storybook/core-common/webpack": "^5",
    "@storybook/core-server/webpack": "^5",
    "@storybook/react/webpack": "^5"
  }
}
